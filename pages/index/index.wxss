/**index.wxss**/
.index_swiper swiper {
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.cates_vav_box {
  display: flex;
}
.cates_vav_box .cates_vav {
  display: flex;
  flex: 1;
}
.cates_vav_box .cates_vav image {
  width: 100%;
  margin: 20rpx;
}
.index_floor .title_floor image {
  width: 100%;
  margin: 15rpx 0;
  margin-bottom: 10rpx;
}
.index_floor .list_floor navigator {
  width: 33.3%;
  float: left;
  box-sizing: border-box;
  /*2 3 超链接*/
}
.index_floor .list_floor navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx #fff solid;
}
.index_floor .list_floor navigator:nth-child(2),
.index_floor .list_floor navigator:nth-child(3) {
  border-bottom: 10rpx #fff solid;
}
.index_floor .list_floor navigator image {
  width: 100%;
  height: 100%;
}
